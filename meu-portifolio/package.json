    "react-bootstrap": "^2.5.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.6.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@sheerun/mutationobserver-shim": "0.3.3",
    "cypress": "10.4.0",
    "cypress-multi-reporters": "1.6.1",
    "eslint-config-trybe-frontend": "^1.3",
    "gh-pages": "^4.0.0",
    "jest-localstorage-mock": "2.4.21",
    "mocha": "10.0.0",
